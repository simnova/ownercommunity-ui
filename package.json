    "craco-less": "^2.1.0-alpha.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "http-server": "^14.1.1",
    "postcss": "^8.4.6",
    "relay-compiler": "^14.1.0",
    "storybook-addon-customize-antd-theme": "^1.1.0",
    "tailwindcss": "^3.0.18",
    "vite": "^4.3.9",
    "vite-plugin-relay": "^2.0.0",
    "webpack": "^5.68.0"
  },
  "relay": {
    "src": "./src",
    "eagerEsModules": true,
    "language": "typescript",
    "schema": "./schema.graphql",
    "schemaExtensions": [
      "../data-access/graphql/schema/core/",
      "../data-access/graphql/schema/interfaces/",
      "../data-access/graphql/schema/shared/",
      "../data-access/graphql/schema/types/"
    ],
    "customScalars": {
      "ObjectID": "string"
    },
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
}
